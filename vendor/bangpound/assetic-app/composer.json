{
    "name": "bangpound/assetic-app",
    "license": "MIT",
    "type": "project",
    "description": "Command line tool for Assetic",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "require": {
        "php": ">=5.3.3",
        "pimple/pimple": "~2.1",
        "symfony/config": "~2.3",
        "symfony/console": "~2.3",
        "symfony/debug": "~2.3",
        "symfony/finder": "~2.3",
        "symfony/yaml": "~2.3",
        "cilex/console-service-provider": "2.*@dev",
        "kriswallsmith/assetic": "~1.2@dev",
        "zacoza/assetic-filters": "dev-master",
        "bangpound/assetic-codekit": "1.0.*@dev"
    },
    "bin": ["bin/assetic"],
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
