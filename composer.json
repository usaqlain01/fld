{
    "require": {
        "symfony/property-access": "~2.0",
        "php": ">=5.3.0",
        "stojg/crop": "~1.2.6",
        "bangpound/crop-extension": "dev-master",
        "symfony/yaml": "2.4.2"
    },
    "config": {
        "autoloader-suffix": "ComposerManager",
        "preferred-install": "dist",
        "vendor-dir": "docroot/../vendor"
    },
    "autoload": {
        "psr-0": {
            "Drupal\\bangpoundmedia\\": [
                "docroot/profiles/fieldmuseum/modules/bangpound/bangpoundmedia/lib/"
            ]
        },
        "psr-4": {
            "": [
                "docroot/profiles/fieldmuseum/lib/"
            ],
            "Drupal\\inline\\": [
                "docroot/profiles/fieldmuseum/modules/contrib/inline/lib/Drupal/inline/"
            ]
        }
    },
    "require-dev": {
        "symfony/console": "~2.4",
        "symfony/debug": "~2.4",
        "symfony/finder": "~2.4",
        "phing/phing": "~2.9@dev",
        "cac/autophing": "~1.3.3",
        "mongolito404/phingdrushtask": "7.1.*@dev",
        "pear-pear.php.net/VersionControl_Git": "*",
        "pimple/pimple": "~3.0",
        "bangpound/assetic-cli": "~1.1@dev",
        "bangpound/assetic-service-provider": "~1.0@dev",
        "zacoza/assetic-filters": "dev-master",
        "bangpound/assetic-codekit": "1.0.*@dev"
    },
    "repositories": [
        {
            "type": "pear",
            "url": "http://pear.php.net"
        },
        {
            "type": "package",
            "package": {
                "name": "mongolito404/phingdrushtask",
                "version": "7.1.x-dev",
                "source": {
                    "url": "git://git.drupal.org/project/phingdrushtask.git",
                    "type": "git",
                    "reference": "7.x-1.x"
                },
                "autoload": {
                    "classmap": [
                        "DrushTask.php"
                    ]
                }
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}