{
    "name": "derhasi/drupal-composer-project",
    "description": "Composer template for Drupal projects",
    "require": {
        "bangpound/crop-extension": "dev-master",
        "composer/installers": "^1.0.20",
        "cweagans/composer-patches": "~1.0",
        "drupal/acquia_cloud_sticky_sessions": "1.*",
        "drupal/acquia_connector": "2.*",
        "drupal/admin_views": "1.*",
        "drupal/auto_entitylabel": "1.*",
        "drupal/better_formats": "1.*",
        "drupal/classloader": "1.*",
        "drupal/composer_manager": "1.*",
        "drupal/ctools": "^1.9",
        "drupal/date": "2.*",
        "drupal/diff": "3.*",
        "drupal/disqus": "1.*",
        "drupal/domain_301_redirect": "^1.2",
        "drupal/draggableviews": "2.*",
        "drupal/editableviews": "1.0.0-beta10",
        "drupal/email": "1.*",
        "drupal/entity": "^1.6",
        "drupal/entity_view_mode": "1.*",
        "drupal/entityqueue": "1.*",
        "drupal/entityreference": "^1.1",
        "drupal/environment_indicator": "2.*",
        "drupal/eva": "1.*",
        "drupal/fast_404": "1.*",
        "drupal/features": "2.*",
        "drupal/feeds": "2.0-beta2",
        "drupal/fences": "1.*",
        "drupal/field_formatter_class": "1.*",
        "drupal/field_formatter_settings": "1.*",
        "drupal/field_group": "1.x-dev",
        "drupal/fieldable_panels_panes": "1.10",
        "drupal/file_entity": "2.*",
        "drupal/globalredirect": "1.x-dev",
        "drupal/google_analytics": "2.*",
        "drupal/image_resize_filter": "1.*",
        "drupal/imagecache_token": "^1.0",
        "drupal/imagemagick": "1.*",
        "drupal/inline": "2.x-dev",
        "drupal/job_scheduler": "2.x-dev",
        "drupal/jquery_update": "2.*",
        "drupal/libraries": "^2.2",
        "drupal/link": "1.*",
        "drupal/markdown": "^1.2",
        "drupal/media": "2.*",
        "drupal/media_gallery": "2.*",
        "drupal/memcache": "1.*",
        "drupal/menu_attributes": "^1.0",
        "drupal/menu_block": "2.*",
        "drupal/menu_position": "1.*",
        "drupal/metatag": "^1.7",
        "drupal/migrate": "2.*",
        "drupal/mollom": "2.*",
        "drupal/multiform": "1.*",
        "drupal/navbar": "^1.6",
        "drupal/node_convert": "1.*",
        "drupal/oembed": "1.x-dev",
        "drupal/optimizely": "2.*",
        "drupal/override_node_options": "1.*",
        "drupal/panelizer": "^3.3",
        "drupal/panels": "^3.6",
        "drupal/pathauto": "1.*",
        "drupal/pathologic": "^3.1",
        "drupal/pdf": "^1.6",
        "drupal/pdf_to_imagefield": "3.*",
        "drupal/plupload": "1.*",
        "drupal/profile2": "1.x-dev",
        "drupal/readmorecontrol": "1.x-dev",
        "drupal/redirect": "1.*",
        "drupal/remote_stream_wrapper": "1.*",
        "drupal/schemaorg": "1.*",
        "drupal/securelogin": "^1.5",
        "drupal/semantic_panels": "1.*",
        "drupal/stage_file_proxy": "1.*",
        "drupal/strongarm": "2.x-dev",
        "drupal/token": "1.*",
        "drupal/token_formatters": "1.*",
        "drupal/transliteration": "3.*",
        "drupal/views": "3.*",
        "drupal/views_bulk_operations": "3.*",
        "drupal/webform": "^4.14",
        "drupal/workbench": "1.*",
        "drupal/workbench_moderation": "1.*",
        "drupal/wysiwyg": "2.x-dev",
        "drupal/xmlsitemap": "2.*",
        "drupal/zen": "5.5",
        "stojg/crop": "dev-master",
        "symfony/filesystem": "~2.3",
        "symfony/yaml": "~2.3",
        "wikimedia/composer-merge-plugin": "^1.3"
    },
    "replace": {
        "drupal/drupal": "*",
        "drupal/aggregator": "*",
        "drupal/block": "*",
        "drupal/blog": "*",
        "drupal/book": "*",
        "drupal/color": "*",
        "drupal/comment": "*",
        "drupal/contact": "*",
        "drupal/contextual": "*",
        "drupal/dashboard": "*",
        "drupal/dblog": "*",
        "drupal/field": "*",
        "drupal/field_sql_storage": "*",
        "drupal/list": "*",
        "drupal/number": "*",
        "drupal/options": "*",
        "drupal/text": "*",
        "drupal/field_ui": "*",
        "drupal/file": "*",
        "drupal/filter": "*",
        "drupal/forum": "*",
        "drupal/help": "*",
        "drupal/image": "*",
        "drupal/locale": "*",
        "drupal/menu": "*",
        "drupal/node": "*",
        "drupal/openid": "*",
        "drupal/overlay": "*",
        "drupal/path": "*",
        "drupal/php": "*",
        "drupal/poll": "*",
        "drupal/profile": "*",
        "drupal/rdf": "*",
        "drupal/search": "*",
        "drupal/shortcut": "*",
        "drupal/statistics": "*",
        "drupal/syslog": "*",
        "drupal/system": "*",
        "drupal/taxonomy": "*",
        "drupal/toolbar": "*",
        "drupal/tracker": "*",
        "drupal/translation": "*",
        "drupal/trigger": "*",
        "drupal/update": "*",
        "drupal/user": "*",
        "drupal/minimal": "*",
        "drupal/standard": "*",
        "drupal/bartik": "*",
        "drupal/garland": "*",
        "drupal/seven": "*",
        "drupal/stark": "*",
        "drupal/rebuild-field_ui": "*"
    },
    "autoload": {
        "psr-4": {
            "": "lib/",
            "Drupal\\inline\\": "modules/contrib/inline/lib/Drupal/inline/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/7"
        }
    ],
    "config": {
        "sort-packages": true,
        "vendor-dir": "../../../lib/default/vendor"
    },
    "extra": {
        "installer-paths": {
            "modules/contrib/{$name}": ["type:drupal-module"],
            "themes/{$name}": ["type:drupal-theme"]
        },
        "patches": {
            "drupal/semantic_panels": {
                "Link classes and attributes array don't work <https://www.drupal.org/node/2106643>": "https://www.drupal.org/files/semantic_panels.code_.2106643-1.patch"
            },
            "drupal/pdf_to_imagefield": {
                "PDF to image file support": "https://gist.githubusercontent.com/bangpound/fb2b942df603c13692d5/raw/0ff1443afed46ae8b183b1f1b1358d6c551a1d09/pdf_to_image_file_support.patch"
            },
            "drupal/inline": {
                "Work on all text fields": "https://gist.githubusercontent.com/bangpound/73e57ffeed3051d06cb7/raw/9ccac28340468cf1d1a0d55a94b247a6f9888871/inline-work_on_all_text_fields.patch"
            },
            "drupal/link": {
                "Link insecure SVG": "https://gist.githubusercontent.com/bangpound/df89f3401c9dbde94e2b/raw/14de9a37317d1429b421e91bac699985bf270f49/link-insecure-svg.patch",
                "Allow underscores in class": "https://gist.githubusercontent.com/bangpound/df89f3401c9dbde94e2b/raw/14de9a37317d1429b421e91bac699985bf270f49/allow-underscores-in-class.patch"
            }
        },
        "merge-plugin": {
            "include": [
                "modules/**/**/composer.json"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
