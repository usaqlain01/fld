{
    "require": {
        "stojg/crop": "dev-develop",
        "bangpound/crop-extension": "dev-master",
        "symfony/yaml": "2.4.2"
    },
    "require-dev": {
        "symfony/console": "~2.4",
        "symfony/debug": "~2.4",
        "symfony/finder": "~2.4",
        "phing/phing": "~2.9@dev",
        "cac/autophing": "~1.3.3",
        "mongolito404/phingdrushtask": "7.1.*@dev",
        "pear-pear.php.net/VersionControl_Git": "*",
        "pimple/pimple": "~3.0",
        "bangpound/assetic-cli": "~1.1@dev",
        "bangpound/assetic-service-provider": "~1.0@dev",
        "zacoza/assetic-filters": "dev-master",
        "bangpound/assetic-codekit": "1.0.*@dev"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/bangpound/crop.git"
        },
        {
            "type": "pear",
            "url": "http://pear.php.net"
        },
        {
            "type": "package",
            "package": {
                "name": "mongolito404/phingdrushtask",
                "version": "7.1.x-dev",
                "source": {
                    "url": "git://git.drupal.org/project/phingdrushtask.git",
                    "type": "git",
                    "reference": "7.x-1.x"
                },
                "autoload": {
                    "classmap": ["DrushTask.php"]
                }
            }
        }
    ],
    "autoload": {
        "psr-4": {
            "": "lib/",
            "Drupal\\inline\\": "modules/contrib/inline/lib/Drupal/inline/"
        }
    }
}
