<?php

/**
 * @file
 * Bulk export of file_type objects generated by Bulk export module.
 */

/**
 * Implements hook_file_default_types().
 */
function file_model_file_default_types() {
  $file_types = array();

  $file_type = new stdClass();
  $file_type->disabled = FALSE; /* Edit this to true to make a default file_type disabled initially */
  $file_type->api_version = 1;
  $file_type->type = 'model';
  $file_type->label = 'Model';
  $file_type->description =  t('A <em>Model</em> file is a 3D image.');
  $file_type->mimetypes = array(
    0 => 'model/*',
    1 => 'video/quicktime',
  );
  $file_types['model'] = $file_type;

  return $file_types;
}
