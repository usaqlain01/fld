<?php

/**
 * @file
 * Bulk export of entitylist_lists objects generated by Bulk export module.
 */

/**
 * Implements hook_entitylist_defaults().
 */
function bangpoundefq_entitylist_defaults() {
  $lists = array();

  $list = new EntityList();
  $list->disabled = FALSE; /* Edit this to true to make a default list disabled initially */
  $list->api_version = 1;
  $list->name = 'promoted_nodes';
  $list->title = 'Promoted nodes';
  $list->description = 'Nodes promoted to front page sorted by sticky and created date.';
  $list->content_type = 0;
  $list->handler_plugin = 'EntityListHandlerEFQ';
  $list->handler_config = array(
    'class_name' => 'PromotedNodeListEFQ',
  );
  $lists['promoted_nodes'] = $list;

  $list = new EntityList();
  $list->disabled = FALSE; /* Edit this to true to make a default list disabled initially */
  $list->api_version = 1;
  $list->name = 'terms';
  $list->title = 'Terms';
  $list->description = 'Terms in a vocbulary sorted by weight.';
  $list->content_type = 0;
  $list->handler_plugin = 'EntityListHandlerEFQ';
  $list->handler_config = array(
    'class_name' => 'TermListEFQ',
  );
  $lists['term_list'] = $list;

  return $lists;
}
